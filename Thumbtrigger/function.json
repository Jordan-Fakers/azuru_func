{
    "bindings": [
      {
        "name": "myBlob",
        "type": "blobTrigger",
        "direction": "in",
        "path": "images/{name}",
        "connection": "AzureWebJobsStorage"
      },
      {
        "name": "outputBlob",
        "direction": "out",
        "type": "blob",
        "path": "images-thumb/{name}_thumb",
        "connection": "AzureWebJobsStorage"
      }
    ]
  }